/* Mixins */
html::before {
  display: none;
  content: "XS,S,M,L,XL";
}
html::after {
  display: none;
  content: "XS";
}
@media screen and (min-width: 480px) {
  html::after {
    content: "S";
  }
}
@media screen and (min-width: 720px) {
  html::after {
    content: "M";
  }
}
@media screen and (min-width: 1024px) {
  html::after {
    content: "L";
  }
}
@media screen and (min-width: 1280px) {
  html::after {
    content: "XL";
  }
}

