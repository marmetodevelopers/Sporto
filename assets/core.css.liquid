{% comment %}
All fonts should be relative to the base font size of the page, so we have to calculate
some relative sizing for the other font base sizes.
{% endcomment %}

{% assign base_size_float = 16.0 %}

{% assign font_body_ratio = settings.font_body_size | divided_by: base_size_float | round: 4 %}
{% assign font_heading_ratio = settings.font_heading_size | divided_by: base_size_float | round: 4 %}
:root {
  --font-size-base: 16px;
  {% comment %} Body Fonts {% endcomment %}
  --font-body-family: {{ fonts_font_body_family }};
  --font-body-style: {{ fonts_font_body_style }};
  --font-body-weight: {{ fonts_font_body_weight }};
  --font-body-weight-bolder: {{ fonts_font_body_weight_bolder }};
  --font-body-size-25: {{ font_body_ratio | times: 0.8125 | append: 'rem' }};
  --font-body-size-50: {{ font_body_ratio | times: 0.875 | append: 'rem' }};
  --font-body-size-75: {{ font_body_ratio | times: 0.9375 | append: 'rem' }};
  --font-body-size-100: {{ font_body_ratio | append: 'rem' }};
  --font-body-size-200: {{ font_body_ratio | times: 1.125 | append: 'rem' }};
  --font-body-size-300: {{ font_body_ratio | times: 1.25 | append: 'rem' }};
  --font-body-size-400: {{ font_body_ratio | times: 1.5 | append: 'rem' }};
  --font-body-size-500: {{ font_body_ratio | times: 1.625 | append: 'rem' }};
  --font-body-size-600: {{ font_body_ratio | times: 1.75 | append: 'rem' }};
  --font-body-size-700: {{ font_body_ratio | times: 2 | append: 'rem' }};
  --font-body-size-800: {{ font_body_ratio | times: 2.25 | append: 'rem' }};
  --font-body-line-height: {{ settings.font_body_height }};
  --font-body-letter-spacing: 0;
  {% comment %} Heading Fonts {% endcomment %}
  --font-heading-family: {{ fonts_font_heading_family }};
  --font-heading-style: {{ fonts_font_heading_style }};
  --font-heading-weight: {{ fonts_font_heading_weight }};
  --font-heading-size-1: {{ font_heading_ratio | times: 1.222 | append: 'rem' }};
  --font-heading-size-2: {{ font_heading_ratio | append: 'rem' }};
  --font-heading-size-3: {{ font_heading_ratio | times: 0.778 | append: 'rem' }};
  --font-heading-size-4: {{ font_heading_ratio | times: 0.667 | append: 'rem' }};
  --font-heading-size-5: {{ font_heading_ratio | times: 0.555 | append: 'rem' }};
  --font-heading-size-6: {{ font_heading_ratio | times: 0.5 | append: 'rem' }};
  --font-heading-size-collapsible-tab: {{ font_heading_ratio | times: 0.555 | append: 'rem' }};
  --font-heading-size-section: {{ font_heading_ratio | times: 1.389 | append: 'rem' }};
  --font-heading-size-page-title: {{ font_heading_ratio | times: 1.389 | append: 'rem' }};
  --font-heading-size-banner: {{ font_heading_ratio | times: 1.667 | append: 'rem' }};
  {% assign font_heading_transform = 'none' %}
  {% if settings.font_heading_uppercase %}
    {% assign font_heading_transform = 'uppercase' %}
  {% endif %}
  --font-heading-line-height: {{ settings.font_heading_height }};
  --font-heading-letter-spacing: {{ settings.font_heading_spacing | divided_by: 100.0 | append: 'em' }};
  --font-heading-transform: {{ font_heading_transform }};
  {% comment %} Subheading Fonts {% endcomment %}
  --font-subheading-family: var(--font-body-family);
  --font-subheading-style: var(--font-body-style);
  --font-subheading-weight: var(--font-body-weight);
  --font-subheading-weight-bolder: var(--font-body-weight-bolder);
  --font-subheading-size-small: var(--font-body-size-200);
  --font-subheading-size-medium: var(--font-body-size-300);
  --font-subheading-size-large: var(--font-body-size-400);
  --font-subheading-line-height: var(--font-body-line-height);
  --font-subheading-letter-spacing: var(--font-body-letter-spacing);
  {% comment %} Accent Fonts {% endcomment %}
  --font-accent-size-small: var(--font-body-size-25);
  --font-accent-size-medium: var(--font-body-size-50);
  --font-accent-size-large: var(--font-body-size-75);
  --font-accent-line-height: 1.2;
  --font-accent-letter-spacing: 0.075em;
  {% comment %} Button Fonts {% endcomment %}
  --font-button-family: var(--font-body-family);
  --font-button-size-small: var(--font-body-size-100);
  --font-button-size-medium: var(--font-body-size-200);
  --font-button-size-large: var(--font-body-size-300);
  --font-button-weight: var(--font-body-weight);
  --font-button-line-height: var(--font-body-line-height);
  --font-button-transform: none;
  --font-button-letter-spacing: 0;
  {% comment %} Signpost Fonts {% endcomment %}
  --font-signpost-size: var(--font-body-size-300);
  --font-signpost-weight: 700;
  --font-signpost-transform: uppercase;
  --font-signpost-letter-spacing: 0.05em;
  {% comment %} Form Fonts {% endcomment %}
  --font-form-label-size: var(--font-body-size-100);
  --font-form-label-weight: var(--font-body-weight);
  --font-form-label-transform: none;
  --font-form-label-letter-spacing: 0;
  --font-form-value-size: var(--font-body-size-100);
  --font-form-value-weight: var(--font-body-weight);
  --font-form-label-info-size: var(--font-body-size-75);
  --font-form-label-message-size: var(--font-body-size-50);
  {% comment %} Product Item Fonts {% endcomment %}
  --font-product-item-name-size: var(--font-body-size-100);
  --font-product-item-name-family: var(--font-body-family);
  --font-product-item-name-weight: var(--font-body-weight);
  --font-product-item-vendor-size: var(--font-body-size-50);
  --font-product-item-price-size: var(--font-body-size-100);
  --font-product-item-price-size-unit: var(--font-body-size-50);
  --font-product-item-price-size-savings: var(--font-body-size-50);
  /* We need to reduce the font sizes on the product cards on mobile */
}
@media screen and (max-width: 719px) {
  :root {
    --font-product-item-name-size: calc(var(--font-body-size-100) * 0.9285);
    --font-product-item-vendor-size: calc(var(--font-body-size-50) * 0.9285);
    --font-product-item-price-size: calc(var(--font-body-size-100) * 0.9285);
    --font-product-item-price-size-unit: calc(var(--font-body-size-50) * 0.9285);
    --font-product-item-price-size-savings: calc(var(--font-body-size-50) * 0.9285);
  }
}

