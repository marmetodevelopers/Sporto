<footer
  data-component="footer"
  data-component-type="section"
  {% if tag != blank %}data-component-tag="{{ tag }}"
  {% endif %}class="footer"
  style="
  --color-footer-background: {{ settings.color_footer_background }};
  --block-text-color: {{ settings.color_footer_text }};
  "
>
  <section-wrapper>
    <section-content>
      <div class="footer__wrapper">
        <div class="footer-top">
          {% for block in section.blocks %}
            {%- comment -%}
              Between 720px and 1024px, the width of the column changes based on these rules:
              1. Between 15% and 33% = 1 column
              2. Between 34% and 66% = 2 columns
              3. 67% and up = 3 columns
            {%- endcomment -%}
            {% liquid
              assign block_width = 'one-column'
              if block.settings.column_width > 33
                if block.settings.column_width > 66
                  assign block_width = 'three-column'
                else
                  assign block_width = 'two-column'
                endif
              endif
            %}
            <div
              class="
                footer-block
                footer-block--size-{{ block_width }}
              "
              style="--column-width: {{ block.settings.column_width }}%;"
            >
              {% case block.type %}
                {% when 'footer_email_form' %}
                  {% comment %}Component block-footer-newsletter-form:default begin{% endcomment %}
                  <div data-component="block-footer-newsletter-form" data-component-type="block" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="block-footer-newsletter-form" {{ block.shopify_attributes }}>
                    {% if block.settings.heading != blank %}
                      <h2 class="block-footer-newsletter-form__heading">
                        {{ block.settings.heading | escape }}
                      </h2>
                    {% endif %}
                  
                    {% if block.settings.content != blank %}
                      <div class="block-footer-newsletter-form__content">
                        {{ block.settings.content }}
                      </div>
                    {% endif %}
                  
                    {% liquid
                      assign label_text = 'navigation.footer.newsletter_form.label' | t
                      assign button_text = 'navigation.footer.newsletter_form.button_label' | t
                      assign placeholder_text = 'navigation.footer.newsletter_form.label' | t
                    %}
                  
                    {%- capture input_attributes -%}
                      required
                      autocorrect="off"
                      autocapitalize="off"
                      autocomplete="email"
                      aria-required="true"
                      {% if form.errors %}
                        autofocus
                        aria-invalid="true"
                        aria-describedby="newsletter-error--{{ section.id }}"
                      {% elsif form.posted_successfully? %}
                        aria-describedby="newsletter-success--{{ section.id }}"
                      {% endif %}
                    {%- endcapture -%}
                  
                    {%- capture button_attributes -%}
                      aria-label="{{ button_label }}"
                      data-newsletter-submit
                    {%- endcapture -%}
                    {% comment %}UUID newsletter_id begin{% endcomment %}
                    {% assign newsletter_id = '7fe34ffa-deb3-4627-9284-f72afee8066e' %}
                    {% comment %}UUID newsletter_id end{% endcomment %}
                    {% assign newsletter_id = newsletter_id | append: '-' | append: block.id %}
                    {% form 'customer', id: newsletter_id, class: 'footer-newsletter-form', data-newsletter: '' %}
                      {% if form.errors %}
                        {% capture form_error_content %}
                          <p
                            id="newsletter-error--{{ section.id }}"
                            tabindex="-1"
                            autofocus
                          >
                            {{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}
                          </p>
                        {% endcapture %}
                  
                        {% comment %}Component notification:default begin{% endcomment %}
                        {% liquid
                          assign content_60186656 = content
                          assign content = form_error_content
                          assign type_60186656 = type
                          assign type = 'error'
                        %}
                        {%- comment -%}
                          @param content [Required]
                            The content that goes in the notification banner
                        
                          @param type [Required]
                            The type of notification banner ('error' or 'success')
                        
                          @param hidden [Optional]
                            Whether the notification should be hidden
                        {%- endcomment -%}
                        
                        <div
                          data-component="notification"
                          data-component-type="component"
                          {% if tag != blank %}data-component-tag="{{ tag }}"
                          {% endif %}class="
                            notification
                            notification--{{ type }}
                          "
                          {% if type == 'error' %}
                            role="alert"
                          {% else %}
                            role="status"
                          {% endif %}
                          {% if hidden %}hidden{% endif %}
                        >
                          {{ content }}
                        </div>
                        
                        {% liquid
                          assign content = content_60186656
                          assign content_60186656 = nil
                          assign type = type_60186656
                          assign type_60186656 = nil
                        %}
                        {% comment %}Component notification:default end{% endcomment %}
                      {% endif %}
                      {%- if form.posted_successfully? -%}
                        {% capture form_success_content %}
                          <h3
                            id="newsletter-success--{{ section.id }}"
                            tabindex="-1"
                            autofocus
                          >
                            {{ 'general.components.newsletter_form.success' | t }}
                          </h3>
                        {% endcapture %}
                        {% comment %}Component notification:default begin{% endcomment %}
                        {% liquid
                          assign content_60186656 = content
                          assign content = form_success_content
                          assign type_60186656 = type
                          assign type = 'success'
                        %}
                        {%- comment -%}
                          @param content [Required]
                            The content that goes in the notification banner
                        
                          @param type [Required]
                            The type of notification banner ('error' or 'success')
                        
                          @param hidden [Optional]
                            Whether the notification should be hidden
                        {%- endcomment -%}
                        
                        <div
                          data-component="notification"
                          data-component-type="component"
                          {% if tag != blank %}data-component-tag="{{ tag }}"
                          {% endif %}class="
                            notification
                            notification--{{ type }}
                          "
                          {% if type == 'error' %}
                            role="alert"
                          {% else %}
                            role="status"
                          {% endif %}
                          {% if hidden %}hidden{% endif %}
                        >
                          {{ content }}
                        </div>
                        
                        {% liquid
                          assign content = content_60186656
                          assign content_60186656 = nil
                          assign type = type_60186656
                          assign type_60186656 = nil
                        %}
                        {% comment %}Component notification:default end{% endcomment %}
                      {%- endif -%}
                      <div class="block-footer-newsletter-form__form-content">
                        <input type="hidden" name="contact[tags]" value="newsletter">
                        {% assign input_id = section.id | append: '-' | append: block.id | append: '-newsletter' %}
                        {% comment %}Component input-field:default begin{% endcomment %}
                        {% liquid
                          assign input_type_87b57e3a = input_type
                          assign input_type = 'email'
                          assign placeholder_87b57e3a = placeholder
                          assign placeholder = placeholder_text
                          assign form_namespace_87b57e3a = form_namespace
                          assign form_namespace = 'contact'
                          assign field_name_87b57e3a = field_name
                          assign field_name = 'email'
                          assign attributes_87b57e3a = attributes
                          assign attributes = input_attributes
                        %}
                        {% comment %}
                          @param form [Required]
                            The form that the input lives in.
                        
                          @param form_namespace [Required]
                            The form's namespace that is used in "name" attribute as form_namespace[field_name].
                        
                          @param field_name [Required]
                            The field name for the form. Will be used for the "name" attribute.
                        
                          @param input_type [Required]
                            The type of field that is being rendered. Usually text, password, number, etc.
                        
                          @param input_id [Optional]
                            The type of field that is being rendered. Usually text, password, number, etc.
                        
                          @param label_text [Required]
                            The text for the label to be included.
                        
                          @param placeholder [Optional]
                            The placeholder for the input field.
                        
                          @param attributes [Optional]
                            Additional attributes to append to the input field.
                        {% endcomment %}
                        
                        {% if input_id == blank %}
                          {% comment %}UUID input_id begin{% endcomment %}
                          {% assign input_id = '641070bc-4748-4cab-a6e1-376b9f3ac3e8' %}
                          {% comment %}UUID input_id end{% endcomment %}
                        {% endif %}
                        
                        <div data-component="input-field" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="input-field">
                          <label for="{{ input_id }}">{{ label_text }}</label>
                          <input
                            id="{{ input_id }}"
                            type="{{ input_type }}"
                            name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
                            value="{{ form[field_name] }}"
                            placeholder="{{ placeholder }}"
                            {{ attributes }}
                          >
                        </div>
                        
                        {% liquid
                          assign input_type = input_type_87b57e3a
                          assign input_type_87b57e3a = nil
                          assign placeholder = placeholder_87b57e3a
                          assign placeholder_87b57e3a = nil
                          assign form_namespace = form_namespace_87b57e3a
                          assign form_namespace_87b57e3a = nil
                          assign field_name = field_name_87b57e3a
                          assign field_name_87b57e3a = nil
                          assign attributes = attributes_87b57e3a
                          assign attributes_87b57e3a = nil
                        %}
                        {% comment %}Component input-field:default end{% endcomment %}
                        {% comment %}Component button:default begin{% endcomment %}
                        {% liquid
                          assign style_a44a8a9e = style
                          assign style = 'primary'
                          assign size_a44a8a9e = size
                          assign size = 'small'
                          assign text_a44a8a9e = text
                          assign text = button_text
                          assign attributes_a44a8a9e = attributes
                          assign attributes = button_attributes
                        %}
                        {% comment %}
                          @param style [Required]
                            This is the style of the button: primary, secondary, tertiary, or text. Default primary.
                        
                          @param size [Required]
                            This defines the size of the button: small, medium, or large. Default medium.
                        
                          @param text [Required]
                            This is the text to be included in the button.
                        
                          @param attributes [Optional]
                            This will include any additional attributes that the button needs. For example, aria-controls.
                        {% endcomment %}
                        
                        <button
                          data-component="button"
                          data-component-type="component"
                          {% if tag != blank %}data-component-tag="{{ tag }}"
                          {% endif %}class="button button--{{ style }} button--{{ size }}{% if style == 'primary' or style == 'secondary' %} button--shadow{% endif %%}"
                          {{ attributes }}
                        >
                          {{ text }}
                        </button>
                        
                        {% liquid
                          assign style = style_a44a8a9e
                          assign style_a44a8a9e = nil
                          assign size = size_a44a8a9e
                          assign size_a44a8a9e = nil
                          assign text = text_a44a8a9e
                          assign text_a44a8a9e = nil
                          assign attributes = attributes_a44a8a9e
                          assign attributes_a44a8a9e = nil
                        %}
                        {% comment %}Component button:default end{% endcomment %}
                      </div>
                    {% endform %}
                  </div>
                  
                  {% comment %}Component block-footer-newsletter-form:default end{% endcomment %}
                {% when 'menu' %}
                  {% comment %}Component block-footer-menu:default begin{% endcomment %}
                  <div data-component="block-footer-menu" data-component-type="block" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="block-footer-menu" {{ block.shopify_attributes }}>
                    {% if block.settings.heading != blank %}
                      <h2 class="block-footer-menu__heading">
                        {{ block.settings.heading | escape }}
                      </h2>
                    {% endif %}
                    {% if block.settings.linklist != blank %}
                      <div class="block-footer-menu__content">
                        <ul class="block-footer-menu__menu">
                          {% for link in linklists[block.settings.linklist].links %}
                            <li class="block-footer-menu__list-item">
                              <a class="block-footer-menu__link" href="{{ link.url }}">
                                {{ link.title | escape }}
                              </a>
                            </li>
                          {% endfor %}
                        </ul>
                      </div>
                    {% endif %}
                  </div>
                  
                  {% comment %}Component block-footer-menu:default end{% endcomment %}
                {% when 'about' %}
                  {% comment %}Component block-footer-about:default begin{% endcomment %}
                  <div data-component="block-footer-about" data-component-type="block" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="block-footer-about" {{ block.shopify_attributes }}>
                    {% if block.settings.heading != blank %}
                      <h2 class="block-footer-about__heading">
                        {{ block.settings.heading | escape }}
                      </h2>
                    {% endif %}
                  
                    {% if block.settings.content != blank %}
                      <div class="block-footer-about__content">
                        {{ block.settings.content }}
                      </div>
                    {% endif %}
                    {% if block.settings.show_social_icons %}
                      {% comment %}Component social-media-links:default begin{% endcomment %}
                      {% comment %}
                        @param use_mobile_spacing {Boolean}} [Optional]
                          Changes the spacing to allow for bigger touch targets on mobile if true.
                      {% endcomment %}
                      
                      {% assign use_mobile_spacing = use_mobile_spacing | default: false %}
                      
                      <ul
                        data-component="social-media-links"
                        data-component-type="component"
                        {% if tag != blank %}data-component-tag="{{ tag }}"
                        {% endif %}class="
                          social-media-links
                          {% if use_mobile_spacing %}
                            social-media-links--mobile-spacing
                          {% endif %}
                        "
                      >
                        {% if settings.social_behance_link != '' %}
                          <li class="social-media-links__item">
                            <a
                              class="social-media-links__link"
                              href="{{ settings.social_behance_link }}"
                              aria-label="{{ shop.name }} on Behance"
                              rel="me"
                              target="_blank"
                            >
                              {% comment %}Icon behance:default begin{% endcomment %}
                              <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M13.805 4.789V3.89h-3.608v.898h3.608ZM6.762 7.68c.26-.122.491-.296.681-.51.291-.376.438-.844.415-1.319.013-.489-.13-.97-.41-1.371-.458-.623-1.234-.943-2.328-.96H.8v8.96h4.035c.425.003.85-.038 1.266-.122A2.465 2.465 0 0 0 7.746 11.2c.267-.426.405-.92.398-1.424a2.366 2.366 0 0 0-.35-1.314 1.98 1.98 0 0 0-1.032-.782ZM2.866 5.075h1.668c.36-.01.72.037 1.066.141.32.133.48.41.48.827 0 .376-.122.64-.365.787a1.79 1.79 0 0 1-.947.223H2.866V5.075Zm2.801 5.691c-.287.12-.597.174-.907.16H2.866v-2.39h1.92c.306-.011.61.04.896.152.38.173.57.494.57.96.004.552-.193.923-.585 1.118Zm9.475-2.576a3.152 3.152 0 0 0-.512-1.328 2.583 2.583 0 0 0-1.107-.942 3.665 3.665 0 0 0-1.509-.3c-.94 0-1.706.296-2.296.89-.59.595-.884 1.448-.883 2.56 0 1.188.326 2.045.978 2.572.637.52 1.438.8 2.26.788 1.036 0 1.841-.312 2.415-.936.371-.393.578-.779.622-1.16h-1.713a1.3 1.3 0 0 1-1.28.735 1.69 1.69 0 0 1-.917-.25c-.427-.259-.65-.71-.672-1.355H15.2a8.238 8.238 0 0 0-.058-1.274Zm-1.684.16H10.56c.04-.37.2-.717.453-.99a1.335 1.335 0 0 1 1.001-.368c.363-.011.717.112.994.347a1.402 1.402 0 0 1 .45 1.015V8.35Z" fill="currentColor"/></svg>
                              {% comment %}Icon behance:default end{% endcomment %}
                            </a>
                          </li>
                        {% endif %}
                      
                        {% if settings.social_clubhouse_link != '' %}
                          <li class="social-media-links__item">
                            <a
                              class="social-media-links__link"
                              href="{{ settings.social_clubhouse_link }}"
                              aria-label="{{ shop.name }} on Clubhouse"
                              rel="me"
                              target="_blank"
                            >
                              {% comment %}Icon clubhouse:default begin{% endcomment %}
