            class="
              image-banner__image
              image-banner__image--mobile
            "
            data-image-banner-image
          >
            {% comment %}Component lazy-img:default begin{% endcomment %}
            {% render 'lazy-img', img: section.settings.mobile_image, lazy: true, canvas: true, focal_point: section.settings.mobile_image.presentation.focal_point %}
