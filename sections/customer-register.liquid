          assign form_namespace_f470a551 = nil
          assign field_name = field_name_f470a551
          assign field_name_f470a551 = nil
          assign attributes = attributes_f470a551
          assign attributes_f470a551 = nil
        %}
        {% comment %}Component input-field:default end{% endcomment %}

        {% assign label_text = 'general.customer.password' | t %}
        {% assign placeholder = '' %}
        {% capture field_attributes %}
          aria-required="true"
          {% if form.errors contains 'email' %}
            aria-invalid="true"
          {% endif %}
        {% endcapture %}
        {% comment %}Component input-field:default begin{% endcomment %}
        {% liquid
          assign input_type_f470a551 = input_type
          assign input_type = 'password'
          assign form_namespace_f470a551 = form_namespace
          assign form_namespace = 'customer'
          assign field_name_f470a551 = field_name
          assign field_name = 'password'
          assign attributes_f470a551 = attributes
          assign attributes = field_attributes
        %}
        {% comment %}
          @param form [Required]
            The form that the input lives in.
        
          @param form_namespace [Required]
            The form's namespace that is used in "name" attribute as form_namespace[field_name].
        
          @param field_name [Required]
            The field name for the form. Will be used for the "name" attribute.
        
          @param input_type [Required]
