<section data-component="customer-register" data-component-type="section" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="customer-register">
  <section-wrapper width="constrained">
    <section-header>
      <h1>{{ 'customer.register.title' | t }}</h1>
    </section-header>
  </section-wrapper>

  <section-wrapper width="constrained">
    <section-content>
      {% form 'create_customer', novalidate: 'novalidate', class: 'customer-create' %}
        {% if form.errors %}
          <div id="account-register-error" tabindex="-1" autofocus>
            {{ form.errors | default_errors }}
          </div>
        {% endif %}

        {% assign label_text = 'general.customer.first_name' | t %}
        {% comment %}Component input-field:default begin{% endcomment %}
        {% liquid
          assign input_type_f470a551 = input_type
          assign input_type = 'text'
          assign form_namespace_f470a551 = form_namespace
          assign form_namespace = 'customer'
          assign field_name_f470a551 = field_name
          assign field_name = 'first_name'
          assign placeholder_f470a551 = placeholder
          assign placeholder = label_text
        %}
        {% comment %}
          @param form [Required]
            The form that the input lives in.
        
          @param form_namespace [Required]
            The form's namespace that is used in "name" attribute as form_namespace[field_name].
        
          @param field_name [Required]
            The field name for the form. Will be used for the "name" attribute.
        
          @param input_type [Required]
            The type of field that is being rendered. Usually text, password, number, etc.
        
          @param input_id [Optional]
            The type of field that is being rendered. Usually text, password, number, etc.
        
          @param label_text [Required]
            The text for the label to be included.
        
          @param placeholder [Optional]
            The placeholder for the input field.
        
          @param attributes [Optional]
            Additional attributes to append to the input field.
        {% endcomment %}
        
        {% if input_id == blank %}
          {% comment %}UUID input_id begin{% endcomment %}
          {% assign input_id = 'ae22de8f-70ea-423c-a66d-1144eac54e3c' %}
          {% comment %}UUID input_id end{% endcomment %}
        {% endif %}
        
        <div data-component="input-field" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="input-field">
          <label for="{{ input_id }}">{{ label_text }}</label>
          <input
            id="{{ input_id }}"
            type="{{ input_type }}"
            name="{% if form_namespace %}{{ form_namespace }}[{{ field_name }}]{% else %}{{ field_name }}{% endif %}"
            value="{{ form[field_name] }}"
            placeholder="{{ placeholder }}"
            {{ attributes }}
          >
        </div>
        
        {% liquid
          assign input_type = input_type_f470a551
          assign input_type_f470a551 = nil
          assign form_namespace = form_namespace_f470a551
          assign form_namespace_f470a551 = nil
          assign field_name = field_name_f470a551
          assign field_name_f470a551 = nil
          assign placeholder = placeholder_f470a551
          assign placeholder_f470a551 = nil
        %}
        {% comment %}Component input-field:default end{% endcomment %}

        {% assign label_text = 'general.customer.last_name' | t %}
        {% comment %}Component input-field:default begin{% endcomment %}
        {% liquid
          assign input_type_f470a551 = input_type
          assign input_type = 'text'
          assign form_namespace_f470a551 = form_namespace
          assign form_namespace = 'customer'
          assign field_name_f470a551 = field_name
          assign field_name = 'last_name'
          assign placeholder_f470a551 = placeholder
          assign placeholder = label_text
        %}
        {% comment %}
          @param form [Required]
            The form that the input lives in.
        
          @param form_namespace [Required]
            The form's namespace that is used in "name" attribute as form_namespace[field_name].
        
          @param field_name [Required]
