        <div data-component="customer-address-form" data-component-type="component" {% if tag != blank %}data-component-tag="{{ tag }}" {% endif %}class="customer-address-form">
          {% capture edit_attributes %}
            aria-label="{{ edit_text }} {{ index }}" aria-expanded="false" aria-controls="edit-address-{{ address.id }}"
          {% endcapture %}
        
          {% capture delete_attributes %}
            aria-label="{{ delete_text }} {{ index }}"
            data-target="{{ address.url }}"
            data-confirm-message="{{ 'customer.addresses.delete_confirm' | t }}"
